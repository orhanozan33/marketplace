{
  "name": "marketplace-workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Marketplace System - Workspace Root",
  "workspaces": [
    "frontend",
    "backend",
    "server"
  ],
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "preview": "cd frontend && npm run preview",
    "start": "cd frontend && npm run start",
    "lint": "cd frontend && npm run lint",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "server:check": "powershell -ExecutionPolicy Bypass -File check-and-start-server.ps1",
    "server:start": "powershell -ExecutionPolicy Bypass -File start-server.ps1",
    "server:install": "cd server && npm install",
    "backend": "cd backend && npm run start:dev",
    "backend:build": "cd backend && npm run build",
    "backend:prod": "cd backend && npm run start:prod",
    "backend:install": "cd backend && npm install",
    "frontend:install": "cd frontend && npm install",
    "backup": "powershell -ExecutionPolicy Bypass -File backup-to-github.ps1",
    "backup:git": "git add . && git commit -m \"Auto backup\" && git push origin main",
    "setup": "powershell -ExecutionPolicy Bypass -File setup.ps1",
    "setup:manual": "npm install && cd frontend && npm install && cd ../server && npm install && cd ../backend && npm install",
    "build:all": "cd frontend && npm run build && cd ../backend && npm run build",
    "install:all": "cd frontend && npm install && cd ../server && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "marketplace",
    "react",
    "nestjs",
    "express"
  ],
  "author": "",
  "license": "UNLICENSED"
}
